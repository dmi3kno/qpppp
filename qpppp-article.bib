  year = {2009},
  journal = {International Statistical Review},
  volume = {77},
  number = {1},
  pages = {40--50},
  issn = {1751-5823},
  doi = {10.1111/j.1751-5823.2009.00076.x},
  urldate = {2022-11-17},
  abstract = {Although the kurtosis index proposed by Karl Pearson in 1905 is introduced in statistical textbooks at all levels, the measure is not easily interpreted and has been a subject of considerable debate. In this study, the theoretical development of kurtosis is surveyed from a historical perspective of Pearson's work on evolution. It surprisingly emerges that there was no emphasis in Pearson's papers on kurtosis as measuring (in part) tail heaviness. However, it is found that Pearson used to frequently adjust the formalisation of kurtosis depending on his changing needs. This complex development partly explains the confusion that would surround kurtosis in subsequent literature. Our conclusion is that most misunderstandings arise from improper use of the kurtosis coefficient outside the Pearson system of frequency curves.},
  langid = {english},
  keywords = {Excess,Flat-toppedness,Pearson system,Standard fourth moment,Tailedness},
  file = {/home/dm0737pe/MEGA/ZoteroLibrary/fiori_zenga_2009_karl_pearson_and_the_origin_of_kurtosis.pdf;/home/dm0737pe/Zotero/storage/425U8GGD/j.1751-5823.2009.00076.html}
}

